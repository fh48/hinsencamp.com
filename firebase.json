{
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/article/how-to-design-and-build-a-carousel-in-VueJS/",
        "destination": "/article/vuejs-carousel/",
        "type": 301
      },

      {
        "source": "/posts/:id*",
        "destination": "/article/:id/",
        "type": 301
      },
      {
        "source": "/users",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/users/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/projects",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/projects/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/uploads/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/jobs",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/jobs/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/links",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/links/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/ueber-uns/",
        "destination": "/team/",
        "type": 301
      },
      {
        "source": "/schulungen/inhouse/",
        "destination": "/schulungen/react-intensiv/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "**/feed.xml",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/rss+xml"
          }
        ]
      },
      {
        "source": "**/code/*",
        "headers": [
          {
            "key": "X-Robots-Tag",
            "value": "noindex, follow"
          }
        ]
      }
    ]
  }
}
